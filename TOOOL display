void welcomeText (){
  display.clearDisplay();
  display.setTextSize(2);
  display.setCursor(0,0);
  display.println("Welcome to   TOOOL");
  //display.setTextSize(1.5);
  display.println("  Kansas     City");
  display.display();
}

void logoDisplay() {
  display.clearDisplay();
  display.drawBitmap(0, 0, image_data_TOOOLarray, 128, 64, 1);
  display.display();
}

void bobDisplay() {
  display.clearDisplay();
  display.drawBitmap(0, 0, seckc_data_BOBarray, 128, 64, 1);
  display.display();
}

void setup() {
  // put your setup code here, to run once:
Serial.begin(115200);

if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
  Serial.println("SSD1306 allocation failed");
  for(;;); //Don't proceed, loop forever.
}
  
  display.clearDisplay();

//display.setTextSize(2);
display.setTextColor(WHITE);
}
void loop() {
  // put your main code here, to run repeatedly:
  logoDisplay();
  delay(5000);
  welcomeText();
  delay(5000);
  bobDisplay();
  delay(5000);
}
